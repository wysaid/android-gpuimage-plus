{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Cleanup workspace (git clean -ffdx)",
            "type": "shell",
            "command": "git",
            "args": [
                "clean",
                "-ffdx"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "[Debug] Build Android Demo",
            "type": "shell",
            "command": "bash",
            "args": [
                "tasks.sh",
                "--debug",
                "--build"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "[Debug] Run Android Demo",
            "type": "shell",
            "command": "bash",
            "args": [
                "tasks.sh",
                "--debug",
                "--run"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "[Debug] Build And Run Android Demo",
            "type": "shell",
            "command": "bash",
            "args": [
                "tasks.sh",
                "--debug",
                "--build",
                "--run"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "[Release] Build Android Demo",
            "type": "shell",
            "command": "bash",
            "args": [
                "tasks.sh",
                "--release",
                "--build"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "[Release] Run Android Demo",
            "type": "shell",
            "command": "bash",
            "args": [
                "tasks.sh",
                "--release",
                "--run"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "[Release] Build And Run Android Demo",
            "type": "shell",
            "command": "bash",
            "args": [
                "tasks.sh",
                "--release",
                "--build",
                "--run"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "[Release] NDK Build Native Library With FFmpeg (Deprecated)",
            "type": "shell",
            "command": "bash",
            "args": [
                "library/src/main/jni/buildJNI"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "[Release] NDK Build Native Library Without FFmpeg (Deprecated)",
            "type": "shell",
            "command": "bash",
            "args": [
                "library/src/main/jni/buildJNI",
                "--disable-video-module",
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "Enable Gradle CMake Build",
            "type": "shell",
            "command": "bash",
            "args": [
                "tasks.sh",
                "--enable-cmake"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "Enable Video Module",
            "type": "shell",
            "command": "bash",
            "args": [
                "tasks.sh",
                "--enable-video-module"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "Disable Video Module",
            "type": "shell",
            "command": "bash",
            "args": [
                "tasks.sh",
                "--disable-video-module"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "Setup Gradle Project",
            "type": "shell",
            "command": "bash",
            "args": [
                "tasks.sh",
                "--setup-project"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "[Debug] Enable CMake And Build Project With CMake",
            "type": "shell",
            "command": "bash",
            "args": [
                "tasks.sh",
                "--debug",
                "--enable-cmake",
                "--build"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "[Release] Enable CMake And Build Project With CMake",
            "type": "shell",
            "command": "bash",
            "args": [
                "tasks.sh",
                "--release",
                "--enable-cmake",
                "--build"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "[Release] Publish AAR To Maven Local - Normal",
            "type": "shell",
            "command": "bash",
            "args": [
                "tasks.sh",
                "--enable-cmake",
                "--enable-video-module",
                "--publish",
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "[Release] Publish AAR To Maven Local - Disable Video Module",
            "type": "shell",
            "command": "bash",
            "args": [
                "tasks.sh",
                "--enable-cmake",
                "--disable-video-module",
                "--publish",
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": "$gcc"
        }
    ]
}